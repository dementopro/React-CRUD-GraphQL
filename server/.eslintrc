{
  "parser": "babel-eslint",
  "rules": {
    "arrow-parens": [2, "always", { "requireForBlockBody": true }],
    "func-names": 0,
    "max-len": [1, 120],
    "object-curly-newline": 0,
    "react/prefer-stateless-function": 0,
    "react/prop-types": 0,
    "react/jsx-filename-extension": 1
  },
  "extends": ["airbnb"],
  "plugins": ["import", "jsx-a11y", "react"],
  "env": {
    "browser": true
  }
}
